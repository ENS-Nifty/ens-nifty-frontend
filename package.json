    "moment": "^2.22.2",
    "netlify-lambda": "^0.4.0",
    "npm-run-all": "^4.1.3",
    "portis": "^1.2.11",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "readline": "^1.3.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
